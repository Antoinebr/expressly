{
  "name": "@fastly/expressly",
  "version": "1.0.0-alpha.5",
  "description": "Express-style router for Fastly's Compute@Edge.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/fastly/expressly"
  },
  "bugs": {
    "url": "https://github.com/fastly/expressly/issues"
  },
  "scripts": {
    "build": "tsc",
    "prepublish": "tsc",
    "dev": "nodemon --exec \"tsc\""
  },
  "keywords": [
    "fastly",
    "c@e",
    "compute",
    "edge",
    "router"
  ],
  "engines": {
    "node": "^16"
  },
  "author": "oss@fastly.com",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^17.0.40",
    "auto": "^10.37.1",
    "husky": "^8.0.1",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "ts-loader": "^9.3.0",
    "typescript": "^4.7.3",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@fastly/js-compute": "^0.2.4",
    "cookie": "^0.5.0",
    "core-js": "^3.22.8",
    "path-to-regexp": "^6.2.1"
  },
  "auto": {
    "baseBranch": "main",
    "plugins": [
      "npm",
      "released"
    ],
    "release": {
      "prerelease": true
    },
    "prereleaseBranches": [
      "alpha",
      "beta",
      "main"
    ]
  }
}
