{
  "name": "@fastly/expressly",
  "version": "2.0.0",
  "description": "Express-style router for Fastly's Compute@Edge.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/fastly/expressly"
  },
  "bugs": {
    "url": "https://github.com/fastly/expressly/issues"
  },
  "scripts": {
    "build": "tsc",
    "prepublish": "tsc",
    "dev": "tsc --watch"
  },
  "keywords": [
    "fastly",
    "c@e",
    "compute",
    "edge",
    "router"
  ],
  "engines": {
    "node": ">=18"
  },
  "author": "oss@fastly.com",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.17.1",
    "auto": "^10.46.0",
    "husky": "^8.0.3",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@fastly/js-compute": "^3.1.1",
    "cookie": "^0.5.0",
    "core-js": "^3.32.0",
    "path-to-regexp": "^6.2.1"
  },
  "auto": {
    "baseBranch": "main",
    "plugins": [
      "npm",
      "released"
    ],
    "release": {
      "prerelease": true
    },
    "prereleaseBranches": [
      "alpha",
      "beta",
      "main"
    ]
  }
}
